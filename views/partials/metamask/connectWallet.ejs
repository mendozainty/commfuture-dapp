<script>
  const getAccount =  async () => {
    try {
      let result;
      const currentAccount = await ethereum.request({
        method: 'eth_accounts'
    })
    result = currentAccount;
    $.post('/auth/dashboard/:userid/web3account', {web3Account: currentAccount}, (result))
    return result;
    } catch (e) {
      $('#web3account').text(e.message);;
    }
    return currentAccount;   
  }
  getAccount()
</script>
